}

AdministrationNotSelectedConditional : WOConditional {
    condition = administrationSelected;
    negate = true;
}

AdministrationSelectedConditional : WOConditional {
    condition = administrationSelected;
}

UserInfoNotSelectedConditional : WOConditional {
    condition = userInfoSelected;
    negate = true;
}

WebsiteRequestsSelectedConditional : WOConditional {
    condition = websiteRequestSelected;
}

WebsiteRequestsNotSelectedConditional : WOConditional {
    condition = websiteRequestSelected;
    negate = true;
}

UserInfoSelectedConditional : WOConditional {
    condition = userInfoSelected;
}

SupportNotSelectedConditional : WOConditional {
    condition = supportSelected;
    negate = true;
}

SupportSelectedConditional : WOConditional {
    condition = supportSelected;
}

UserNameString: WOString {
