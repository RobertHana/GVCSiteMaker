OrderingTextField: WOTextField {
    value = selectedIndex;
    onBlur = orderingTextFieldOnBlur;
    style = "width:40px; text-align:center;";
    numberformat = "0";
    onKeyPress="return blurOnEnter(this,event)";
}

TRGenericContainer : WOGenericContainer {
    elementName = "div";
    style = "display: none;";
    id = reorderBusyDivID;
}

BusyImage : WOImage {
    framework = "Ajax";
    filename = "wait30.gif";
}
