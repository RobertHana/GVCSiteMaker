package jass.examples;

public class Producer extends Thread {

  protected Buffer pbuffer;
  
  public Producer (Buffer buffer) {
    this.pbuffer = buffer;
  }
    
  public synchronized void run () { 
    for (int i = 0;i<100;) {
      if (!pbuffer.full()) {
        System.out.println("--> "+i);       
        pbuffer.add(new Integer(i++));
      }
    }
  }
  
}