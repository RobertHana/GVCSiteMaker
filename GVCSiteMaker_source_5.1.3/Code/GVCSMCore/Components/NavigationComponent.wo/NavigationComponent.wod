    action = adminMainPage;
}

UserInfoHyperlink : WOHyperlink {
    action = myUserInfoPage;
}

WebsiteRequestHyperlink : WOHyperlink {
    action = websiteRequestPage;
}

SupportHyperlink : WOHyperlink {
    action = supportPage;
}

IsAdminConditional : WOConditional {
    condition = isAdminUser;
}

