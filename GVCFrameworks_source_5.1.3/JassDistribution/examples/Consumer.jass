package jass.examples;

public class Consumer extends Thread {

  protected Buffer cbuffer;
  
  public Consumer (Buffer buffer) {
    this.cbuffer = buffer;
  }
  
  public synchronized void run () {
    for (int i = 0;i < 100;) {
      if (!cbuffer.empty()) {
        System.out.println(cbuffer.remove()+" <--");
        i++;
      }
    }
  }
  
}